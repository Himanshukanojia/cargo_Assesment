<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="92dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.028"
        app:srcCompat="@drawable/img"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/name"
        android:layout_width="122dp"
        android:layout_height="21dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="164dp"
        android:fontFamily="@font/poppins_thin"
        android:text="James Bond"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textFontWeight="700"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/email"
        android:layout_width="186dp"
        android:layout_height="21dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="195dp"
        android:fontFamily="@font/poppins_thin"
        android:text="infoabraham12@gmail.com"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="DuplicateIds" />

    <ImageView
        android:id="@+id/profile"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="56dp"
        android:background="#00000000"
        android:src="@drawable/img_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email" />

    <TextView
        android:id="@+id/my_profile"
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="280dp"
        android:fontFamily="@font/poppins_thin"
        android:gravity="top"
        android:text="My Profile"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="19sp"
        app:layout_constraintLeft_toRightOf="@id/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/my_enquries"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:background="#00000000"
        android:src="@drawable/img_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile" />

    <TextView
        android:id="@+id/My_profile"
        android:layout_width="120dp"
        android:layout_height="24dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="340dp"
        android:fontFamily="@font/poppins_thin"
        android:gravity="top"
        android:text="My Enquries"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="19sp"
        app:layout_constraintLeft_toRightOf="@id/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/inbox"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:background="#00000000"
        android:src="@drawable/img_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_enquries" />

    <TextView
        android:id="@+id/Inbox"
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="400dp"
        android:fontFamily="@font/poppins_thin"
        android:gravity="top"
        android:text="Inbox"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="19sp"
        app:layout_constraintLeft_toRightOf="@id/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/share_app"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:background="#00000000"
        android:src="@drawable/img_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inbox" />

    <TextView
        android:id="@+id/Share_App"
        android:layout_width="120dp"
        android:layout_height="24dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="460dp"
        android:fontFamily="@font/poppins_thin"
        android:gravity="top"
        android:text="Share App"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="19sp"
        app:layout_constraintLeft_toRightOf="@id/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/world_map"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:background="#00000000"
        android:src="@drawable/img_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/share_app" />

    <TextView
        android:id="@+id/World_Map"
        android:layout_width="120dp"
        android:layout_height="24dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="520dp"
        android:fontFamily="@font/poppins_thin"
        android:gravity="top"
        android:text="World Map"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="19sp"
        app:layout_constraintLeft_toRightOf="@id/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/settings"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:background="#00000000"
        android:src="@drawable/img_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/world_map" />

    <TextView
        android:id="@+id/Settings"
        android:layout_width="120dp"
        android:layout_height="24dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="580dp"
        android:fontFamily="@font/poppins_thin"
        android:gravity="top"
        android:text="Settings"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="19sp"
        app:layout_constraintLeft_toRightOf="@id/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/logout"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="28dp"
        android:background="#00000000"
        android:src="@drawable/img_7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/settings" />

    <TextView
        android:id="@+id/Logout"
        android:layout_width="120dp"
        android:layout_height="24dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="648dp"
        android:fontFamily="@font/poppins_thin"
        android:gravity="top"
        android:text="Logout"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="19sp"
        app:layout_constraintLeft_toRightOf="@id/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/plane"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppins_thin"
        android:text="Plane Cargo"
        android:textColor="@color/white"
        android:textFontWeight="700"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/logout" />

    <TextView
        android:id="@+id/sea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppins_thin"
        android:text="Sea Cargo"
        android:textColor="@color/white"
        android:textFontWeight="700"
        app:layout_constraintStart_toEndOf="@id/plane"
        app:layout_constraintTop_toBottomOf="@id/logout" />



</androidx.constraintlayout.widget.ConstraintLayout>