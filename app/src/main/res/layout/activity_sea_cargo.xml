        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginStart="120dp"
        android:layout_marginEnd="120dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
