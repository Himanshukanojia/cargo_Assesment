        android:inputType="textPersonName"
        android:text=""
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="@id/rectangle_1"
        app:layout_constraintTop_toBottomOf="@+id/cbm"
        tools:ignore="MissingConstraints" />

    <View
